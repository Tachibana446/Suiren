<Window x:Class="Suiren.Logout"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Suiren"
        mc:Ignorable="d"
        Title="Logout" Height="300" Width="300">
    <WrapPanel Orientation="Vertical" HorizontalAlignment="Center">
        <ComboBox x:Name="comboBox" FontSize="24"></ComboBox>
        <Button x:Name="logoutButton" Margin="0,10" Click="logoutButton_Click"
                FontSize="24">ログアウト</Button>
    </WrapPanel>
</Window>
