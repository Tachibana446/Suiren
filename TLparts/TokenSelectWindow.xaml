<Window x:Class="Suiren.TLparts.TokenSelectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Suiren.TLparts"
        mc:Ignorable="d"
        Title="使用するアカウントを選択" Height="300" Width="350" Icon="/Suiren;component/170411SuirenIcon.ico">
    <DockPanel>
        <WrapPanel DockPanel.Dock="Bottom" HorizontalAlignment="Right">
            <WrapPanel.Resources>
                <Style TargetType="Button"  >
                    <Setter Property="FontSize" Value="24" />
                    <Setter Property="Padding"  Value="10,0" />
                    <Setter Property="Margin" Value="0,0,10,0" />
                </Style>
            </WrapPanel.Resources>
            <Button x:Name="okButton" Click="okButton_Click">OK</Button>
            <Button x:Name="cancelButton" Click="cancelButton_Click">キャンセル</Button>
        </WrapPanel>
        <ListBox x:Name="listBox">

        </ListBox>
    </DockPanel>
</Window>
